algoritmo "Soma"
var
   N1,N2: Inteiro
   M: Real

inicio
   Escreval ("MÉDIA ENTRE DOIS NÚMEROS")
   Escreval ("-------------------------")
   Escreva ("Informe um número (N1): ")
   Leia (N1)
   Escreva ("Informe outro número (N2): ")
   Leia (N2)
   M <- (N1 + N2) / 2
   Escreva ("A média aritmética entre ", N1, " e ", N2, " é igual a: ", M)
fimalgoritmo


